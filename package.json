{
  "private": true,
  "scripts": {
    "g:tsc": "cd $INIT_CWD && del dist/ && tsc && cpy '**/*' '!**/*.ts' '!**/*.tsx' ../dist/ --cwd=src/ --no-overwrite --parents"
  },
  "devDependencies": {
    "cpy-cli": "3.1.1",
    "del-cli": "3.0.1",
    "typescript": "4.0.5"
  },
  "workspaces": {
    "packages": [
      "src/packages/*",
      "src/client-services/*",
      "src/backend-services/*"
    ]
  },
  "name": "microservices-starter"
}
